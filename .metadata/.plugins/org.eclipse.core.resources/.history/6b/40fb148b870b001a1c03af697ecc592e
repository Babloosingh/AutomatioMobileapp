package learnjava;

public class myfirsttest {
	
	   public void eat() {
		      System.out.println("I can eat");
		   }
		   public void sleep() {
		      System.out.println("I can sleep");
		   }
		}
		class Dog extends myfirsttest {
		   public void bark() {
		      System.out.println("I can bark");
		   }
		}
  
	}			  

	

